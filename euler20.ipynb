    "import math\n",
    "\n",
    "def euler(x):\n",
    "    r = 0\n",
    "    fat = str(math.factorial(x))\n",
    "    for num in fat:\n",
    "        r += int(num)\n",
    "    return(r)\n",
    "\n",
    "euler(100)"
   ]
